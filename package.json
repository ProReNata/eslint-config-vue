{
  "name": "@prorenata/eslint-config-vue",
  "version": "1.13.16",
  "description": "ProReNata's Vue JS ESLint config.",
  "homepage": "https://github.com/ProReNata/eslint-config-vue",
  "author": {
    "name": "Graham Fairweather",
    "email": "graham.fairweather@prorenata.se"
  },
  "copyright": "Copyright (c) 2018-present",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "find-unused": "eslint-find-rules --unused",
    "prepare": "! type safe-publish-latest >/dev/null 2>&1 || safe-publish-latest && node node_modules/in-publish/in-install.js || (npm test && npm run find-unused && npm run prettier-check)",
    "test": "npm run --silent tests-only",
    "tests-only": "eslint index.js",
    "security": "npm audit",
    "security-fix": "npm run security -- fix",
    "lint": "eslint -f 'node_modules/eslint-friendly-formatter' --ext .js,.json .",
    "lint-fix": "npm run lint -- --fix",
    "prettier-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ProReNata/eslint-config-vue.git"
  },
  "bugs": {
    "url": "https://github.com/ProReNata/eslint-config-vue/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "prorenata",
    "javascript",
    "styleguide"
  ],
  "files": [
    ".eslintignore",
    ".eslintrc.js",
    ".prettierignore",
    ".prettierrc.json",
    "index.js"
  ],
  "dependencies": {
    "@xotic750/eslint-config-recommended": "^1.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sort-class-members": "^1.5.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-vue": "^5.2.3"
  },
  "devDependencies": {
    "@xotic750/eslint-find-rules": "^4.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "in-publish": "^2.0.0",
    "jscodeshift": "^0.6.4",
    "safe-publish-latest": "^1.1.2",
    "webpack": "^4.38.0"
  },
  "peerDependencies": {
    "eslint-friendly-formatter": "^4.0.1",
    "webpack": "^4.38.0"
  },
  "engines": {
    "node": ">=8.11.4",
    "npm": "6.10.1"
  },
  "browserslist": []
}
